{
  "name": "plaid",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node --log-error index.ts",
    "postinstall": "patch-package",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" ./",
    "lint:types": "tsc --noEmit",
    "format": "prettier --write ./**/*.{js,ts,tsx}"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "ejs": "^3.0.1",
    "envvar": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.3",
    "moment": "^2.24.0",
    "patch-package": "^6.2.0",
    "plaid": "^4.3.0",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "husky": "^4.0.7",
    "lint-staged": ">=8",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
